#include <stdio.h>

int
main ()
{

  int n1, n2, n3, n4, n5, sum, diff, average; //decleration of data

/*getting user input*/
  printf ("Enter five numbers:\n"); 
  scanf ("%d", &n1);
  scanf ("%d", &n2);
  scanf ("%d", &n3);
  scanf ("%d", &n4);
  scanf ("%d", &n5);

  sum = n1 + n2 + n3 + n4 + n5; //this line sums all the user input
  diff = sum / 5; //this line divides the total of sum 
  average = diff; //assignment of "average" 
  printf ("average = %d", average); //printing the result

  return 0; //to end the code

}
